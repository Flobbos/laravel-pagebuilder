{
    "name": "flobbos/laravel-pagebuilder",
    "description": "Build content in your database with translations.",
    "keywords": [
        "laravel",
        "pages",
        "database"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexej Krzewitzki",
            "email": "alexej@helloo.it",
            "homepage": "http://helloo.it"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "illuminate/support": "5.6.*|5.7.*|5.8.*|6.*|7.*|8.*",
        "flobbos/laravel-translatable-db": "^1.4",
        "cocur/slugify": "^4.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Flobbos\\Pagebuilder\\PagebuilderServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Flobbos\\Pagebuilder\\": [
                "src/Pagebuilder",
                "src/database/seeds/"
            ]
        },
        "classmap": []
    },
    "minimum-stability": "stable"
}